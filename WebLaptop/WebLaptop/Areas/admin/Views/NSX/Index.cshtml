@model IEnumerable<WebLaptop.Models.NSX>

@{
    ViewBag.Title = "Danh sách nhà sản xuất";
    Layout = "~/Areas/Admin/Views/Shared/Layout_Admin.cshtml";
}

<link rel="stylesheet" href="~/Content/css/admin.css" />
<link rel="stylesheet" href="~/Content/bootstrap.css" />

<ol class="breadcrumb">
    <li class="breadcrumb-item custom-title">Danh sách nhà sản xuất</li>
</ol>
<div class="card mb-3">
    <div class="card-header">
        <i class="fa fa-table"></i> Danh sách nhà sản xuất
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-striped table-hover table-bordered" id="nsx-table">
                <tr>
                    <th>
                        @Html.DisplayName("Mã NSX")
                    </th>
                    <th>
                        @Html.DisplayName("Tên NSX")
                    </th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.maNSX)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tenNSX)
                        </td>
                       
                    </tr>
                }
            </table>
        </div>
    </div>
</div>